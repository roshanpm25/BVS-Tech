{
    "version": 2,
    "builds": [
      {
        "src": "build/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "api/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/form/:formId",
        "destination": "/"
      }
    ]
  }
  